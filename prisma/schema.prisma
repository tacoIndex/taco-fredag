generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model EanResponeDtos {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  payload   Json
}

model Product {
  id       String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  ean       String   @unique
  url       String
  name      String
  extraData String?
  store     String
  created   DateTime
  updated   DateTime
  currentPrice Float
}
